doctype html
html
  head
    title Webinar Request
    = stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" =>  true
    = javascript_include_tag "application", "data-turbolinks-track" =>  true
    = csrf_meta_tags

  body
    div#page
      div#header
        div#logo = link_to 'Webinar Request', root_path

      .lifted#content
        = render partial: 'layouts/menu'
        br
        = yield

      #footer
        #legal &copy; 2014 University of Maryland, Baltimore county